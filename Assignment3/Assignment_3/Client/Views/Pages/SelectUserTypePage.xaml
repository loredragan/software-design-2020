<Page x:Class="Client.Views.SelectUserTypePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Client.Views"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="SelectUserTypePage"
      Background="AliceBlue">

    <StackPanel>
        <Label Margin="340,130,225,0" Content="Select User Type" Height="24"/>
        <Button Click="BtnAdminUser_Click" x:Name="btnAdminUser" Margin="340,20,0,0" Content="Administrator" Width="89" HorizontalAlignment="Left"/>
        <Button Click="BtnRegUser_Click" x:Name="btnRegUser" Margin="340,10,0,0" Content="Regular User" Width="89" HorizontalAlignment="Left"/>
    </StackPanel>
</Page>
